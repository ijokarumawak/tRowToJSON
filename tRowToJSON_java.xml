<?xml version="1.0" encoding="UTF-8"?>
<COMPONENT>
	<HEADER AUTHOR="Me" COMPATIBILITY="ALL" PLATEFORM="ALL"
		RELEASE_DATE="20120905A" SERIAL="" STARTABLE="false" STATUS="ALPHA"
		VERSION="0.2" HAS_CONDITIONAL_OUTPUTS="true" SCHEMA_AUTO_PROPAGATE="false" DATA_AUTO_PROPAGATE="false">
		<SIGNATURE />
	</HEADER>
	<FAMILIES>
		<FAMILY>Processing/Fields</FAMILY>
	</FAMILIES>
	<DOCUMENTATION>
		<URL>https://github.com/ijokarumawak/tRowToJSON</URL>
	</DOCUMENTATION>
	<CONNECTORS>
		<CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="1"
			MIN_INPUT="1" MIN_OUTPUT="1" />
	</CONNECTORS>
	<PARAMETERS>
		<PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" NUM_ROW="10" />
		<PARAMETER NAME="JSON_COLUMN" FIELD="COLUMN_LIST" REQUIRED="true"
			NUM_ROW="1">
		</PARAMETER>
		<PARAMETER NAME="USE_ALL_COLUMN" FIELD="CHECK" NUM_ROW="15" />
		<PARAMETER NAME="EXCLUDE_COLUMNS" FIELD="TABLE" NB_LINES="3"
			NUM_ROW="16" SHOW_IF="USE_ALL_COLUMN == 'true')">
			<ITEMS>
				<ITEM NAME="EXCLUDE_COLUMN" FIELD="PREV_COLUMN_LIST" />
			</ITEMS>
		</PARAMETER>
		<PARAMETER NAME="JSON_FIELDS" FIELD="TABLE" NB_LINES="3"
			NUM_ROW="20" REQUIRED="true" SHOW_IF="USE_ALL_COLUMN == 'false')">
			<ITEMS>
				<ITEM NAME="INPUT_COLUMN" FIELD="PREV_COLUMN_LIST" />
				<ITEM NAME="JSON_FIELD" FIELD="String" />
			</ITEMS>
		</PARAMETER>
	</PARAMETERS>
	<ADVANCED_PARAMETERS />
	<CODEGENERATION>
		<IMPORTS>
			<IMPORT MODULE="jettison-1.1.jar" NAME="jettison" REQUIRED="true" />
			<IMPORT MODULE="stax-api-1.0.1.jar" NAME="stax-api" REQUIRED="true" />
		</IMPORTS>
	</CODEGENERATION>  
	<RETURNS>
      <RETURN NAME="NB_LINE" AVAILABILITY="AFTER" TYPE="id_Integer"/> 
	</RETURNS>
</COMPONENT>

